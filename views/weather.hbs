<!DOCTYPE html>
<html lang="en">

{{>weather_meta}}


<body>
    {{>navbar}}

    <div class="container-fluid ">
        <div class="main_content">
            <form class="temp_form">
                <input type="text" id="cityName" placeholder="Enter your city name" autocomplete="off">
                <input type="submit" value="search" id="submitBtn">
            </form>
        </div>
        <div class="tempInformation">
            <div class="top_layer">
                <p id="day">Weather API</p>
                <p id="today_data">Dynamically Exp_site</p>
            </div>
            <div class="main_layer">

                <p id="city_name">Get output</p>
                <div class="middle_layer">
                    <p id="temp"></p>
                    <p id="temp_status"></p>
                </div>

            </div>
        </div>
    </div>
</body>

{{>footer}}

<script src="js/main.js"></script>

</html>